{
	"name": "@beyond-js/reactive",
	"version": "0.0.3",
	"title": "Reactive",
	"description": "A model for creating reactive objects with events that enable the creation of reactive models based on the Observer pattern.",
	"layout": "main-layout",
	"template": "template/template.json",
	"keywords": [
		"Observer",
		"reactive-model",
		"beyond-js",
		"observable",
		"nodejs",
		"events"
	],
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "./modules"
	},
	"deployment": {
		"build": {
			"path": "./compiled"
		},
		"distributions": [
			{
				"name": "web-typed",
				"platform": "web",
				"environment": "development",
				"ts": {
					"compiler": "tsc"
				},
				"ports": {
					"bundles": 7530
				}
			},
			{
				"name": "web",
				"platform": "web",
				"environment": "development",
				"ports": {
					"bundles": 6530
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/events": "^0.0.6",
		"@beyond-js/kernel": "~0.1.8",
		"@beyond-js/local": "~0.1.3",
		"@beyond-js/react-18-widgets": "~0.0.1",
		"ajv": "^8.12.0",
		"socket.io-client": "^4.5.4"
	},
	"devDependencies": {
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9"
	}
}
